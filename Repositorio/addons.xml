    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.addon.common" version="2.0.0"/>
    <import addon="plugin.video.youtube" version="5.0.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
   <extension point="xbmc.addon.repository" name="Mi repositorio">
        <info>https://raw.githubusercontent.com/keyven-code/repo/master/Repositorio/addons.xml</info>
        <checksum>https://raw.githubusercontent.com/keyven-code/repo/master/Repositorio/addons.xml.md5</checksum>
        <datadir zip="true">https://raw.githubusercontent.com/keyven-code/repo/master/Repositorio/</datadir>
